{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: builddb Debug",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/solution/builddb/bin/Debug/net9.0/builddb.exe",
            "args": [
                "-i",
                "${workspaceFolder}\\audio\\library",
                "-o",
                "${workspaceFolder}\\solution\\builddb\\results\\db"
            ],
            "cwd": "${workspaceFolder}/solution/builddb",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "C#: builddb Build",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build builddb",
            "program": "dotnet",
            "args": [
                "--version"
            ],
            "cwd": "${workspaceFolder}/solution/builddb",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "C#: identify Debug",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/solution/identify/bin/Debug/net9.0/identify.exe",
            "args": [
                "-d",
                "${workspaceFolder}\\solution\\builddb\\results\\db.msgpack",
                "-i",
                "${workspaceFolder}\\audio\\noisy_samples\\23_Zwei_einsame_Seelen_samples_noisy\\23_Zwei_einsame_Seelen_0.wav"
            ],
            "cwd": "${workspaceFolder}/solution/identify",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "C#: identify Build",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build identify",
            "program": "dotnet",
            "args": [
                "--version"
            ],
            "cwd": "${workspaceFolder}/solution/identify",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}